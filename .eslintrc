{
    "asi": 0,
    "expr": 1,
    "loopfunc": 1,
    "curly": 0,
    "evil": 0,
    "laxcomma": 0,
    "white": 0,
    "undef": 0,
    "indent": 2,
    "rules" : {
			"quotes": [0, "single"],
			"no-unused-vars": 0,
			"strict": [0, "never"],
			"no-multi-spaces": 0,
			"no-redeclare" : 0,
			"no-undef": 0,
			"no-shadow": 0,
			"key-spacing": 0
    },
    "env": {
			"browser": true,
      "node": true
    }
  }
